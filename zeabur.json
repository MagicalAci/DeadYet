{
  "$schema": "https://json.schemastore.org/zeabur.json",
  "build_command": "cd Backend && npm install && npm run build",
  "start_command": "cd Backend && npm start",
  "output_dir": "Backend/dist",
  "watch_paths": [
    "Backend/src/**/*",
    "Backend/package.json"
  ],
  "serverless": false,
  "env": {
    "NODE_ENV": "production",
    "PORT": "8080"
  }
}
